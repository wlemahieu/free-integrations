{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000",
  "dependencies": {
    "antd": "^3.26.4",
    "axios": "^0.19.0",
    "connected-react-router": "^6.6.1",
    "lodash": "^4.17.15",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.3.0",
    "react-sound": "^1.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "socket.io-client": "^2.3.0",
    "universal-cookie": "^4.0.3",
    "uuid": "^3.3.3"
  },
  "scripts": {
    "lint": "NODE_ENV=development npx eslint --fix 'src/**/*.{js,jsx}'",
    "lint-check": "NODE_ENV=development npx eslint 'src/**/*.{js,jsx}' -f json -o ./lintout.json",
    "start": "yarn lint-check && PORT=3001 react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "dev": "cd ../server && yarn run dev"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react-app": "^9.1.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-react-app": "^5.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-flowtype": "^4.5.3",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jest": "^23.2.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.17.0",
    "jest": "^24.9.0",
    "jest-prop-type-error": "^1.1.0",
    "react-test-renderer": "^16.12.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "modulePaths": [
      "<rootDir>/src/"
    ],
    "setupFilesAfterEnv": [
      "jest-prop-type-error",
      "<rootDir>/src/jest.setup.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}"
    ],
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  },
  "babel": {
    "env": {
      "test": {
        "plugins": [
          [
            "lodash"
          ],
          [
            "import",
            {
              "libraryName": "antd"
            }
          ]
        ]
      }
    },
    "plugins": [
      [
        "lodash"
      ],
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": true
        }
      ]
    ],
    "presets": [
      [
        "react-app"
      ],
      [
        "@babel/env",
        {
          "targets": {
            "node": 6
          }
        }
      ]
    ]
  }
}
